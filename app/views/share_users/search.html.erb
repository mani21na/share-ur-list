<h1>Search user by username to share "<%= @list.subject %>" list.</h1>

<%= form_tag(new_list_share_user_path, method: :get) do %>
    <%= label_tag(:search, "Search for username you want to share this list with.") %><br>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag("Search") %>
<% end %>
<%= button_to "Cancel", list_path(@list.id), method: :get %>