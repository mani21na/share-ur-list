<%@no = 0%>
<% user_lists.each do |list| %>
    <%@no += 1%>
    <label><%=@no %>.</label><br> 
    <label>Subject: </label><br>
    <%= link_to list.subject, list_path(list.id) %><br>
    
    <% if !list.share_users.empty? && !list.share_users.map{ |user| user.user_id }.include?(params[:user_id]) %>

        <label>Share users: </label><br>

        <ul>
        <% list.share_users.each do |user| %>
            <li><%= user.username  %></li>
        <% end %>
        </ul>
    <% elsif list.share_users.map{ |user| user.user_id == session[:user_id]} %>
        <label>List creator: </label><br>
        <%= list_creater(list.id)%>
    <% end %>
    <br>
    <br>
<%end%>
